{
  "name": "d3-axes",
  "version": "0.5.2",
  "description": "D3 plugin to manage pairs of axes.",
  "main": "d3-axes.js",
  "module": "index",
  "jsnext:main": "index",
  "keywords": [
    "d3",
    "d3-module",
    "axis",
    "axes",
    "chart",
    "plot",
    "graph",
    "data-join"
  ],
  "scripts": {
    "clean": "rm -f d3-axes.js d3-axes.min.js",
    "pretest": "rollup -c",
    "test": "tape 'test/**/*-test.js'",
    "preversion": "npm test",
    "version": "npm run pretest && uglifyjs d3-axes.js -c -m -o d3-axes.min.js",
    "postpublish": "git checkout gh-pages && npm version --no-git-tag-version $(npm view d3-axes version) && git checkout master && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/au-phiware/d3-axes.git"
  },
  "author": "Corin Lawson <corin@phiware.com.au>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/au-phiware/d3-axes/issues"
  },
  "homepage": "https://au-phiware.github.io/d3-axes/",
  "dependencies": {
    "d3-selection": "^1.3.0",
    "d3-compose": "^0.1.1",
    "d3-gup": "^0.2.0"
  },
  "devDependencies": {
    "rollup": "^0.62.0",
    "rollup-plugin-buble": "^0.19.2",
    "tape": "^4.9.1",
    "uglify-js": "^3.4.4"
  }
}
