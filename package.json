{
  "name": "d3-axes",
  "version": "0.1.0",
  "description": "D3 plugin to manage pairs of axes.",
  "main": "d3-axes.js",
  "module": "index",
  "jsnext:main": "index",
  "keywords": [
    "d3",
    "d3-module",
    "axis",
    "axes",
    "chart",
    "plot",
    "graph",
    "data-join"
  ],
  "scripts": {
    "clean": "rm -f d3-axes.js d3-axes.min.js",
    "pretest": "rollup -c",
    "test": "tape 'test/**/*-test.js'",
    "preversion": "npm test",
    "version": "npm run pretest && uglifyjs d3-axes.js -c -m -o d3-axes.min.js && git add d3-axes.js d3-axes.min.js",
    "postpublish": "git checkout gh-pages && npm update d3-axes && git checkout master && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/au-phiware/d3-plot.git"
  },
  "author": "Corin Lawson <corin@phiware.com.au>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/au-phiware/d3-line-chart/issues"
  },
  "homepage": "https://github.com/au-phiware/d3-line-chart#readme",
  "dependencies": {
    "d3-selection": "^1.0.2"
  },
  "devDependencies": {
    "rollup": "^0.36.0",
    "rollup-plugin-buble": "^0.14.0",
    "tape": "^4.6.0",
    "uglify-js": "^2.7.3"
  }
}
